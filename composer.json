{
    "name": "sakadda/cli",
    "description": "Sakadda CLI Tool",
    "keywords": ["sakadda"],
    "license": "MIT",
    "require": {
        "php": ">=7.2.5",
        "guzzlehttp/guzzle": "^6.5.5|^7.5.0",
        "symfony/console": "^4.0|^5.0|^6.2.5",
        "symfony/process": "^4.2|^5.0|^6.2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5.32"
    },
    "bin": [
        "bin/sakadda"
    ],
    "autoload": {
        "psr-4": {
            "Sakadda\\Cli\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sakadda\\Cli\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
