{
    "name": "sakadda/cli",
    "description": "Sakadda CLI Tool",
    "keywords": ["sakadda"],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "illuminate/support": "^10.4.1",
        "guzzlehttp/guzzle": "^7.5.0",
        "symfony/console": "^6.2.7",
        "symfony/process": "^6.2.7",
        "pestphp/pest": "^2.2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0.18"
    },
    "bin": [
        "bin/sakadda"
    ],
    "autoload": {
        "psr-4": {
            "Sakadda\\Cli\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sakadda\\Cli\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "version": "v1.0.4"
}
